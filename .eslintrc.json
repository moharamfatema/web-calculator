{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns":["*.test.js","*.ico","*.svg","*.png"],
    "rules": {
        "no-debugger":"warn",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
